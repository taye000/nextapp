{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "module": "commonjs",
    "typeRoots": ["./src/@types", "./node_modules/@types"],
    "esModuleInterop": true,
    "outDir": "build",
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,

    "strict": true,

    "skipLibCheck": true
  }
}
